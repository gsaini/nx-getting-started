{
  "version": 1,
  "projects": {
    "react-express-app": {
      "root": "apps/react-express-app",
      "sourceRoot": "apps/react-express-app/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/react-express-app",
            "index": "apps/react-express-app/src/index.html",
            "main": "apps/react-express-app/src/main.tsx",
            "polyfills": "apps/react-express-app/src/polyfills.ts",
            "tsConfig": "apps/react-express-app/tsconfig.app.json",
            "assets": [
              "apps/react-express-app/src/favicon.ico",
              "apps/react-express-app/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/react-express-app/src/environments/environment.ts",
                  "with": "apps/react-express-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "react-express-app:build",
            "proxyConfig": "apps/react-express-app/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "react-express-app:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/react-express-app/.eslintrc",
            "tsConfig": [
              "apps/react-express-app/tsconfig.app.json",
              "apps/react-express-app/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/react-express-app/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/react-express-app/jest.config.js",
            "tsConfig": "apps/react-express-app/tsconfig.spec.json"
          }
        }
      }
    },
    "react-express-app-e2e": {
      "root": "apps/react-express-app-e2e",
      "sourceRoot": "apps/react-express-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/react-express-app-e2e/cypress.json",
            "tsConfig": "apps/react-express-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "react-express-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "react-express-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/react-express-app-e2e/.eslintrc",
            "tsConfig": ["apps/react-express-app-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/react-express-app-e2e/**"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/api/.eslintrc",
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json"
          }
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/api-interfaces/.eslintrc",
            "tsConfig": [
              "libs/api-interfaces/tsconfig.lib.json",
              "libs/api-interfaces/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/api-interfaces/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/api-interfaces/jest.config.js",
            "tsConfig": "libs/api-interfaces/tsconfig.spec.json"
          }
        }
      }
    },
